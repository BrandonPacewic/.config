priority 10000
context "math()"
snippet 'sympy(.*)sympy' "Evaluate Sympy Block" wr
`!p
from sympy import *
x, y, z, t = symbols('x y z t')
k, m, n = symbols('k m n', integer=True)
f, g, h = symbols('f g h', cls=Function)
init_printing()
snip.rv = eval('latex(' + match.group(1).replace('\\', '') \
    .replace('^', '**') \
    .replace('{', '(') \
    .replace('}', ')') + ')')
`
endsnippet

snippet plot "Plot / Graph" w
\begin{figure}[!ht]
    \centering
    \begin{tikzpicture}
        \begin{axis}
            \addplot[domain=$1:$2, samples=${5:100}, color=${6:black}]{$7};
        \end{axis}
    \end{tikzpicture}
    \caption{$8}
\end{figure}$0
endsnippet

snippet plotad "Plot / Graph (Advanced)" w
\begin{figure}[!ht]
    \centering
    \begin{tikzpicture}
        \begin{axis}[
            xmin= ${1:-10}, xmax= ${2:10},
            ymin= ${3:-10}, ymax = ${4:10},
            axis lines = middle,
        ]
            \addplot[domain=$1:$2, samples=${5:100}, color=${6:black}]{$7};
        \end{axis}
    \end{tikzpicture}
    \caption{$8}
\end{figure}$0
endsnippet

