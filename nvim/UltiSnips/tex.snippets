global !p
def math():
    return vim.eval('vimtex#syntax#in_mathzone()') == '1'
endglobal

snippet template "Basic Template" b
\documentclass[a4paper]{report}
\input{preamble}

\title{\Huge{$1}\\\\$2}
\author{\huge{Brandon Pacewic}}
\date{$3}

\begin{document}

\maketitle
$4\tableofcontents
\newpage
\pagebreak

$0

\end{document}
endsnippet

snippet mathhw "Math Homework Template" b
\documentclass[a4paper]{report}
\input{preamble}

\pagestyle{fancy}
